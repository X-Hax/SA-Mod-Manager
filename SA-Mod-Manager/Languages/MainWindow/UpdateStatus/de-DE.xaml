<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="UpdateStatus.ChkUpdate">Suche nach Updates...</system:String>
    <system:String x:Key="UpdateStatus.ChkManagerUpdate">Suche nach SA Mod Manager-Updates...</system:String>
    <system:String x:Key="UpdateStatus.ChkLoaderUpdate">Suche nach Mod Loader-Updates...</system:String>
    <system:String x:Key="UpdateStatus.ChkModsUpdates">Suche nach Mod-Updates...</system:String>
    <system:String x:Key="UpdateStatus.ChkCodesUpdates">Suche nach Codes-Updates...</system:String>
    <system:String x:Key="UpdateStatus.ChkPatchesUpdates">Suche nach Patches-Updates...</system:String>
    <system:String x:Key="UpdateStatus.ChkManagerUpdateFail">SA Mod Manager konnte nicht aktualisiert werden</system:String>
    <system:String x:Key="UpdateStatus.ChkLoaderUpdateFail">Mod Loader konnte nicht aktualisiert werden</system:String>
    <!-- '{0}' will be used to display the name of the mod, ex: "Checking for Super Sonic Mod Update..."-->
    <system:String x:Key="UpdateStatus.ChkCurModUpdate">Suche nach Updates für »{0}«...</system:String>
    <system:String x:Key="UpdateStatus.ModUpdateFail">»{0}« konnte nicht aktualisiert werden</system:String>
    <system:String x:Key="UpdateStatus.NoUpdateFound">Kein Mod-Update gefunden</system:String>
    <system:String x:Key="UpdateStatus.InstallLoader">Mod Loader wird installiert, bitte warten...</system:String>
    <system:String x:Key="UpdateStatus.UninstallLoader">Mod Loader wird deinstalliert, bitte warten...</system:String>
    <system:String x:Key="UpdateStatus.InstallDependencies">Abhängigkeiten werden installiert, bitte warten...</system:String>
    <system:String x:Key="UpdateStatus.UpdateCodes">Codes werden aktualisiert, bitte warten...</system:String>
    <system:String x:Key="UpdateStatus.FailedUpdateCodes">Codes konnten nicht aktualisiert werden</system:String>
    <system:String x:Key="UpdateStatus.FailedUpdatePatches">Patches konnten nicht aktualisiert werden</system:String>
    <system:String x:Key="UpdateStatus.UpdateDependencies">Abhängigkeiten werden aktualisiert, bitte warten...</system:String>
    <system:String x:Key="UpdateStatus.FailedUpdateDependencies">Abhängigkeiten konnten nicht aktualisiert werden</system:String>
    <system:String x:Key="UpdateStatus.ManagerUpdate">SA Mod Manager wird aktualisiert...</system:String>
    <system:String x:Key="UpdateStatus.LoaderUpdate">Mod Loader wird aktualisiert...</system:String>
    <system:String x:Key="UpdateStatus.LoaderUpdateManual">Mod Loader lokal aktualisiert.</system:String>
    <system:String x:Key="UpdateStatus.LoaderInstalled">Mod Loader wurde erfolgreich installiert</system:String>
    <system:String x:Key="UpdateStatus.LoaderUninstalled">Mod Loader wurde erfolgreich deinstalliert</system:String>
    <system:String x:Key="UpdateStatus.TotalMods">{0} Mods geladen</system:String>
    <system:String x:Key="UpdateStatus.TotalCodes">{0} Codes geladen</system:String>



</ResourceDictionary>