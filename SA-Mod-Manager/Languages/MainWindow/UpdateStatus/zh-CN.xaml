<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:system="clr-namespace:System;assembly=mscorlib"
>
  <system:String x:Key="UpdateStatus.ChkUpdate">正在检查更新……</system:String>
  <system:String x:Key="UpdateStatus.ChkManagerUpdate">正在检查 SA 模组管理器的更新……</system:String>
  <system:String x:Key="UpdateStatus.ChkLoaderUpdate">正在检查模组加载器更新……</system:String>
  <system:String x:Key="UpdateStatus.ChkModsUpdates">正在检查模组更新……</system:String>
  <system:String x:Key="UpdateStatus.ChkCodesUpdates">正在检查代码更新……</system:String>
  <system:String x:Key="UpdateStatus.ChkPatchesUpdates">正在检查补丁更新……</system:String>
  <system:String x:Key="UpdateStatus.ChkManagerUpdateFail">更新 SA 模组管理器失败</system:String>
  <system:String x:Key="UpdateStatus.ChkLoaderUpdateFail">更新模组加载器失败</system:String>
  <!-- '{0}' will be used to display the name of the mod, ex: "Checking for Super Sonic Mod Update..."-->
  <system:String x:Key="UpdateStatus.ChkCurModUpdate">正在检查 {0} 模组更新……</system:String>
  <system:String x:Key="UpdateStatus.ModUpdateFail">更新 {0} 模组失败</system:String>
  <system:String x:Key="UpdateStatus.NoUpdateFound">未找到模组更新</system:String>
  <system:String x:Key="UpdateStatus.InstallLoader">正在安装模组加载器，请稍候……</system:String>
  <system:String x:Key="UpdateStatus.UninstallLoader">正在卸载模组加载器，请稍候……</system:String>
  <system:String x:Key="UpdateStatus.InstallDependencies">正在安装依赖，请稍候……</system:String>
  <system:String x:Key="UpdateStatus.UpdateCodes">正在更新代码，请稍候……</system:String>
  <system:String x:Key="UpdateStatus.FailedUpdateCodes">更新代码失败</system:String>
  <system:String x:Key="UpdateStatus.FailedUpdatePatches">更新补丁失败</system:String>
  <system:String x:Key="UpdateStatus.UpdateDependencies">正在更新依赖，请稍候……</system:String>
  <system:String x:Key="UpdateStatus.FailedUpdateDependencies">更新依赖失败</system:String>
  <system:String x:Key="UpdateStatus.ManagerUpdate">正在更新 SA 模组管理器……</system:String>
  <system:String x:Key="UpdateStatus.LoaderUpdate">正在更新模组加载器……</system:String>
  <system:String x:Key="UpdateStatus.LoaderUpdateManual">本地更新模组加载器。</system:String>
  <system:String x:Key="UpdateStatus.LoaderInstalled">成功安装模组加载器</system:String>
  <system:String x:Key="UpdateStatus.LoaderUninstalled">成功卸载模组加载器</system:String>
  <system:String x:Key="UpdateStatus.TotalMods">模组 {0} 已读取</system:String>
  <system:String x:Key="UpdateStatus.TotalCodes">代码 {0} 已读取</system:String>
</ResourceDictionary>