<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:system="clr-namespace:System;assembly=mscorlib">

	<system:String x:Key="MessageWindow.DefaultTitle">Gestor de mods de SA</system:String>
	<system:String x:Key="MessageWindow.DefaultTitle.Error">Error</system:String>
	<system:String x:Key="MessageWindow.DefaultTitle.Warning">Advertencia</system:String>
	
	<!--#region Errors-->
	<system:String x:Key="MessageWindow.Errors.ModWithoutFolder0">Hay un archivo mod.ini en la carpeta mods.</system:String>
	<system:String x:Key="MessageWindow.Errors.ModWithoutFolder1" xml:space="preserve">&#x0a;&#x0a;Cada mod debe colocarse en una subcarpeta que se ubique en la carpeta mods. No extraigas mods directamente a la carpeta mods.</system:String>
	<system:String x:Key="MessageWindow.Errors.ModWithoutFolder2" xml:space="preserve">&#x0a;&#x0a;Mueve o borra mod.ini de la carpeta mods e inicia el Gestor de mods de nuevo.</system:String>

	<system:String x:Key="MessageWindow.Errors.ModNotFound" xml:space="preserve">no se encontró.&#x0a;&#x0a;Este mod se borrará de la lista.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.GamePathFailed.Title">Error al establecer la ruta de juego</system:String>
	<system:String x:Key="MessageWindow.Errors.GamePathFailed">No se pudo encontrar sonic.exe, asegúrate de seleccionar una carpeta en la que esté instalado SADX.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.GamePathNotFound.Title">No se detecta la ruta del juego</system:String>
	<system:String x:Key="MessageWindow.Errors.GamePathNotFound">No se encontró el directorio del juego. Por favor, fija la ruta en la Configuración del gestor y asegúrate de elegir una carpeta donde esté instalado sonic.exe.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.ModAlreadyExists.Title">"El mod ya existe"</system:String>
	<system:String x:Key="MessageWindow.Errors.ModAlreadyExists" xml:space="preserve">Ya existe un mod con ese nombre.&#x0a;Por favor, elige un nuevo nombre o renombra el existente.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.ModCreationFailed.Title">"Error de creación de mod"</system:String>
	
	<system:String x:Key="MessageWindow.Errors.NoModName.Title">Nombre del mod inválido</system:String>
	<system:String x:Key="MessageWindow.Errors.NoModName">No puedes tener un mod sin nombre.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.MissingLoaderDLL" xml:space="preserve">No se encuentra el archivo SADXModLoader.dll, el Cargador de mods no funcionará.&#x0a;&#x0a;Por favor, vuelve a descargar el Gestor de mods.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.DontDoThat">Eh... No te recomiendo hacer eso... :3</system:String>
	
	<system:String x:Key="MessageWindow.Errors.InvalidFramerate.Title">Ajuste inválido</system:String>
	<system:String x:Key="MessageWindow.Errors.InvalidFramerate">Se detectó un ajuste de velocidad de fotogramas no válido.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.CodesListFailed.Title">Error al cargar códigos</system:String>
	<system:String x:Key="MessageWindow.Errors.CodesListFailed">Error al cargar la lista de códigos</system:String>
	
	<system:String x:Key="MessageWindow.Errors.CheckUpdateError.Title">Errores al actualizar</system:String>
	<system:String x:Key="MessageWindow.Errors.CheckUpdateError" xml:space="preserve">Se presentaron los siguientes errores al buscar actualizaciones:&#x0a;&#x0a;</system:String>
    <system:String x:Key="MessageWindow.Errors.D3D8Update">No se pudo actualizar d3d8.dll</system:String>
    <system:String x:Key="MessageWindow.Errors.D3D8UpdateCheck">No se pudo verificar la versión de d3d8to9</system:String>

    <system:String x:Key="MessageWindow.Errors.MissingManifest.Title">No se encuentra mod.manifest</system:String>
    <system:String x:Key="MessageWindow.Errors.MissingManifest">Ninguno de los mods seleccionados tiene manifiestos, por lo que no se pueden verificar.</system:String>

    <system:String x:Key="MessageWindow.Errors.ModFailedVerif.Title">Fallo de integridad</system:String>
    <system:String x:Key="MessageWindow.Errors.ModFailedVerif0" xml:space="preserve">Los siguientes mods no han superado la verificación:&#x0a;</system:String>
    <system:String x:Key="MessageWindow.Errors.ModFailedVerif1">archivo(s)</system:String>
    <system:String x:Key="MessageWindow.Errors.ModFailedVerif2" xml:space="preserve">&#x0a;&#x0a;¿Quieres intentar repararlos?</system:String>

    <system:String x:Key="MessageWindow.Errors.URIParseFail.Title">Fallo al analizar URI</system:String>
    <system:String x:Key="MessageWindow.Errors.URIParseFail0">El URI de instalación con un click malformado</system:String>
    <system:String x:Key="MessageWindow.Errors.URIParseFail1" xml:space="preserve">provocó un fallo en el análisis:&#x0a;</system:String>

    <system:String x:Key="MessageWindow.Errors.GBAPIFail.Title">Fallo en la API de GameBanana</system:String>
    <system:String x:Key="MessageWindow.Errors.GBAPIFail">Errores en la consulta a la API de GameBanana:</system:String>

    <system:String x:Key="MessageWindow.Errors.URIParseFailFields0">El URI de instalación con un click</system:String>
    <system:String x:Key="MessageWindow.Errors.URIParseFailFields1">no contiene los campos requeridos.</system:String>

    <system:String x:Key="MessageWindow.Errors.DirectoryDeleteFail.Title">Error al borrar el directorio</system:String>
    <system:String x:Key="MessageWindow.Errors.DirectoryDeleteFail0">Error al eliminar el directorio temporal de las actualizaciones.</system:String>
    <system:String x:Key="MessageWindow.Errors.DirectoryDeleteFail1">¿Quieres volver a intentarlo? Puedes eliminar el directorio manualmente más tarde.</system:String>

    <system:String x:Key="MessageWindow.Errors.DirectoryCreateFail.Title">Error al crear el directorio</system:String>
    <system:String x:Key="MessageWindow.Errors.DirectoryCreateFail0">Error al crear el directorio temporal de las actualizaciones.</system:String>
    <system:String x:Key="MessageWindow.Errors.DirectoryCreateFail1">¿Quieres volver a intentarlo?</system:String>

    <system:String x:Key="MessageWindow.Errors.UpdateFailed.Title">Error al actualizar</system:String>
    <system:String x:Key="MessageWindow.Errors.UpdateFailed">No se pudo actualizar:</system:String>

    <system:String x:Key="MessageWindow.Errors.ModUpdateFailed0">Error al actualizar mod</system:String>
    <system:String x:Key="MessageWindow.Errors.ModUpdateFailed1">Presiona Reintentar para intentarlo de nuevo, o Cancelar para omitir este mod.</system:String>

    <system:String x:Key="MessageWindow.Errors.ManagerUpdateFail">No se ha podido obtener la información de la actualización.</system:String>

    <system:String x:Key="MessageWindow.Errors.Net7Missing">El Gestor de mods de SADX no funciona sin .NET 7.</system:String>

    <system:String x:Key="MessageWindow.Errors.VCMissing.Title">VC++ faltantes</system:String>
    <system:String x:Key="MessageWindow.Errors.VCMissing" xml:space="preserve">Parece que te faltan algunos redistribuibles de Visual C++, algunos mods no funcionarán sin ellos.&#x0a;&#x0a;¿Quieres descargarlos e instalarlos ahora?</system:String>

    <system:String x:Key="MessageWindow.Errors.GenericDLFail.Title">Error al descargar</system:String>
    <system:String x:Key="MessageWindow.Errors.GenericDLFail0">No se pudo descargar</system:String>
    <system:String x:Key="MessageWindow.Errors.GenericDLFail1">¿Quieres volver a intentarlo?</system:String>
	
	<system:String x:Key="MessageWindow.Errors.LoaderFailedToSave.Title">No se guardó la información del Cargador.</system:String>
	<system:String x:Key="MessageWindow.Errors.LoaderFailedToSave">El directorio para los mods no parece ser una ruta válida, por favor asegúrate de que lo sea. La configuración actual del Cargador de mods no se ha guardado.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.CodeFail.Title">Línea de código inválida</system:String>
	<system:String x:Key="MessageWindow.Errors.CodeFailEmptyLine">Hay una línea vacía dentro de las líneas del escritor de código.&#x0a;&#x0a;Por favor, elimina la línea vacía y vuelve a guardar.</system:String>
	<system:String x:Key="Messagewindow.Errors.CodeFailInvalid">El código escrito es inválido.&#x0a;&#x0a;Por favor, corrige el error y vuelve a guardar.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.HealthCheck.UnknownError.Title">Error desconocido</system:String>
	<system:String x:Key="MessageWindow.Errors.HealthCheck.UnknownError">Ocurrió un error desconocido.</system:String>
    <system:String x:Key="MessageWindow.Errors.ModDelete" xml:space="preserve">No se pudo borrar el mod {0}.&#x0a;Por favor, asegúrate de que los archivos existen y de que no están siendo usados por otro programa, luego vuélvelo a intentar.</system:String>
    <system:String x:Key="MessageWindow.Errors.ProfileLoad">An unexpected error occurred while loading profiles, if the problem persists, please report the issue.</system:String>
    <!--#endregion-->
	
	<!--#region Warnings-->
	<system:String x:Key="MessageWindow.Warnings.DeleteMod0" xml:space="preserve">Esto desinstalará todos los mods seleccionados:&#x0a;&#x0a;</system:String>
    <system:String x:Key="MessageWindow.Warnings.DeleteMod1">¿Estás seguro(a) de que quieres continuar?</system:String>
    
    <system:String x:Key="MessageWindow.Warnings.DisableSaveRedirect" xml:space="preserve">Estás a punto de desactivar la redirección de guardado de este mod. Si continúas, todos los archivos guardados en la carpeta de este mod serán eliminados.&#x0a;&#x0a;¿Estás seguro(a) de que quieres continuar?</system:String>
	
	<system:String x:Key="MessageWindow.Warnings.DeleteProfile">¿Seguro(a) que quieres borrar el/los perfil(es) seleccionado(s)?</system:String>
	
	<system:String x:Key="MessageWindow.Warnings.ForceModUpdate" xml:space="preserve">Esto obligará a que todos los mods seleccionados se vuelvan a descargar por completo.&#x0a;&#x0a;¿Estás seguro(a) de que quieres continuar?</system:String>
	
	<system:String x:Key="MessageWindow.Warnings.GenerateManifest" xml:space="preserve">Esto puede causar la PÉRDIDA DE LOS DATOS DEL USUARIO DEL MOD (ARCHIVOS DE GUARDADO, ARCHIVOS DE CONFIGURACIÓN) hasta la próxima actualización. Para evitar esto, nunca debes ejecutar esto en mods que no has desarrollado.&#x0a;&#x0a;¿Estás seguro(a) de que quieres continuar?</system:String>

    <system:String x:Key="MessageWindow.Warnings.Net7Missing" xml:space="preserve">¡.NET 7.0 Desktop Runtime no está instalado!&#x0a;&#x0a;¿Quieres instalarlo ahora?</system:String>
	
	<system:String x:Key="MessageWindow.Warnings.NewCodeDelete" xml:space="preserve">Esto borrará todos los códigos seleccionados.&#x0a;&#x0a;¿Seguro(a) que quieres continuar?</system:String>

	<system:String x:Key="MessageWindow.Warnings.DisableUpdates" xml:space="preserve">Estás a punto de desactivar las actualizaciones de este mod. Si continúas, el Gestor NUNCA revisará si existen actualizaciones de este mod.&#x0a;¿Estás seguro(a) de que quieres continuar?</system:String>

	<system:String x:Key="MessageWindow.Warnings.7zMissing" xml:space="preserve">El Gestor de mods necesita 7-zip para extraer mods y poder actualizarse.&#x0a;&#x0a;¿Quieres instalarlo ahora?</system:String>

    <system:String x:Key="MessageWindow.Warnings.ProfilesDeleted.Title" xml:space="preserve">Perfiles faltantes</system:String>
    <system:String x:Key="MessageWindow.Warnings.ProfilesDeleted.Message1" xml:space="preserve">No se encontraron los siguientes perfiles:</system:String>
    <system:String x:Key="MessageWindow.Warnings.ProfilesDeleted.Message2" xml:space="preserve">Se eliminarán de la lista de Perfiles y el perfil seleccionado se convertirá en el primero de la lista.</system:String>

    <system:String x:Key="MessageWindow.Warnings.ProfileMigration.Title" xml:space="preserve">Error al importar</system:String>
    <system:String x:Key="MessageWindow.Warnings.ProfileMigration.Message1" xml:space="preserve">No se pudieron importar los siguientes archivos como perfiles:</system:String>
    <system:String x:Key="MessageWindow.Warnings.ProfileMigration.Message2" xml:space="preserve">&#x0a;&#x0a;Por favor, asegúrate de que sólo utilizas archivos JSON de perfil de juego o archivos INI del antiguo cargador.</system:String>
    
    <!--#endregion-->
	
	<!--#region Information-->
	<system:String x:Key="MessageWindow.Information.NoAvailableUpdates.Title">No se encontraron actualizaciones de los mods</system:String>
	<system:String x:Key="MessageWindow.Information.NoAvailableUpdates">Todos los mods están actualizados.</system:String>

    <system:String x:Key="MessageWindow.Information.D3D8Update">La versión de d3d8.dll en la carpeta de SADX es diferente a la que se incluye con el Cargador de mods. ¿Quieres actualizar la copia instalada?</system:String>

    <system:String x:Key="MessageWindow.Information.ModPassedVerif.Title">Integridad verficada</system:String>
    <system:String x:Key="MessageWindow.Information.ModPassedVerif">Todos los mods seleccionados pasaron la verificación.</system:String>

    <system:String x:Key="MessageWindow.Information.GameDetected.Title">Juego encontrado</system:String>
    <system:String x:Key="MessageWindow.Information.GameDetected" xml:space="preserve">El juego {0} se encontró en '{1}'&#x0a;&#x0a;¿Quieres descargar e instalar el Cargador de Mods?</system:String>
    <system:String x:Key="MessageWindow.Information.SADXSteamDetected" xml:space="preserve">Se encontró la carpeta del juego SADX, pero parece ser la versión de Steam.&#x0a;El Gestor de mods sólo funciona con la versión para PC de 2004.&#x0a;&#x0a;¿Quieres descargar e instalar el instalador de mods para convertir tu juego a la versión de 2004? (Tus partidas guardadas también se convertirán).</system:String>
    <system:String x:Key="MessageWindow.Information.SADXSteamDetectedTemp" xml:space="preserve">Se encontró la carpeta del juego SADX, pero parece ser la versión de Steam.&#x0a;El Gestor de mods sólo funciona con la versión para PC de 2004.&#x0a;&#x0a;Por favor, descarga e instala el Instalador de Mods de SADX para convertir tu juego a la versión de 2004. (Tus partidas guardadas también se convertirán).</system:String>
    <system:String x:Key="MessageWindow.Warnings.HealthCheckTitle">Revisión de salud</system:String>
	<system:String x:Key="MessageWindow.Warnings.HealthCheck" xml:space="preserve">Estás por ejecutar la verificación de salud del juego. Esto verificará todos los archivos del juego, excepto los archivos de audio.&#x0a;&#x0a;¿Deseas continuar?</system:String>

	<system:String x:Key="MessageWindow.Information.HealthCheck.FailedFiles.Title">Se encontraron problemas con los archivos</system:String>
	<system:String x:Key="MessageWindow.Information.HealthCheck.FailedFiles1">Se encontraron los siguientes problemas con tus archivos del juego:</system:String>
	<system:String x:Key="MessageWindow.Information.HealthCheck.FailedFiles2">Ten en cuenta que si tu juego funciona como se espera, entonces los problemas mencionados no están afectándolo.</system:String>

	<system:String x:Key="MessageWindow.Information.HealthCheck.Verified.Title">Verificación completada</system:String>
	<system:String x:Key="MessageWindow.Information.HealthCheck.Verified">Todos los archivos revisados fueron verificados.</system:String>
    
    <system:String x:Key="MessageWindow.Information.Exception.Title">Incluir ajustes</system:String>
    <system:String x:Key="MessageWindow.Information.Exception.Message">¿Quieres copiar la configuración de tu Gestor y del Perfil en el portapapeles para incluirla en tu informe?&#x0a;&#x0a;No es necesario, pero puede ayudar a resolver problemas.</system:String>

    <system:String x:Key="MessageWindow.Information.BugReport.Title">Reportar error</system:String>
    <system:String x:Key="MessageWindow.Information.BugReport.Message1">Estás por reportar un error, asegúrate de que:</system:String>
    <system:String x:Key="MessageWindow.Information.BugReport.Message2">Tu Gestor esté actualizado.</system:String>
    <system:String x:Key="MessageWindow.Information.BugReport.Message3">Tu Cargador de mods esté actualizado.</system:String>
    <system:String x:Key="MessageWindow.Information.BugReport.Message4">Todos los mods estén actualizados.</system:String>
    <system:String x:Key="MessageWindow.Information.BugReport.Message5">Si ya comprobaste todo lo anterior, sigue las siguientes instrucciones para enviarlo al repositorio correcto:</system:String>
    <system:String x:Key="MessageWindow.Information.BugReport.Message6">Si tienes algún problema con el programa que utilizas actualmente, utiliza el botón Gestor de mods.</system:String>
    <system:String x:Key="MessageWindow.Information.BugReport.Message7">Para cualquier problema con el juego, como congelaciones, bloqueos, etc., utiliza el botón Cargador de mods.</system:String>
    <!--#endregion-->

</ResourceDictionary>