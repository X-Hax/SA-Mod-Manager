<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:system="clr-namespace:System;assembly=mscorlib">

	<system:String x:Key="MessageWindow.DefaultTitle">Gerenciador de Mods SADX</system:String>
	<system:String x:Key="MessageWindow.DefaultTitle.Error">Erro</system:String>
	<system:String x:Key="MessageWindow.DefaultTitle.Warning">Aviso</system:String>
	
	<!--#region Errors-->
	<system:String x:Key="MessageWindow.Errors.ModWithoutFolder0">Há um mod.ini na pasta de mods.</system:String>
	<system:String x:Key="MessageWindow.Errors.ModWithoutFolder1" xml:space="preserve">&#x0a;&#x0a;Cada mod deve ser colocado em uma subpasta dentro da pasta de mods. Não extraia mods diretamente para a pasta de mods.</system:String>
	<system:String x:Key="MessageWindow.Errors.ModWithoutFolder2" xml:space="preserve">&#x0a;&#x0a;Mova ou exclua o mod.ini na pasta de mods e abra o Gerenciador de Mods novamente.</system:String>

	<system:String x:Key="MessageWindow.Errors.ModNotFound" xml:space="preserve">não pôde ser achado.&#x0a;&#x0a;Este mod será removido da lista.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.GamePathFailed.Title">Falha ao definir o Caminho para o Jogo</system:String>
	<system:String x:Key="MessageWindow.Errors.GamePathFailed">Falha ao tentar detectar sonic.exe, tente selecionar uma pasta onde SADX esteja instalado.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.GamePathNotFound.Title">Falha ao detectar o Caminho para o Jogo</system:String>
	<system:String x:Key="MessageWindow.Errors.GamePathNotFound">Falha ao tentar detectar o caminho para a pasta do Jogo. Por favor defina o caminho nas Definições do Gerenciador e selecione uma pasta onde sonic.exe estiver instalado.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.ModAlreadyExists.Title">"Mod já existe"</system:String>
	<system:String x:Key="MessageWindow.Errors.ModAlreadyExists" xml:space="preserve">Um mod com esse nome já existe.&#x0a;Por favor escolha um nome diferente ou renomeie o existente.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.ModCreationFailed.Title">"Erro na Criação do Mod"</system:String>
	
	<system:String x:Key="MessageWindow.Errors.NoModName.Title">Nome de mod inválido</system:String>
	<system:String x:Key="MessageWindow.Errors.NoModName">Você não pode ter um mod sem nome.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.MissingLoaderDLL" xml:space="preserve">O arquivo SADXModLoader.dll não pôde ser encontrado, o Carregador de Mods não irá funcionar.&#x0a;&#x0a;Por favor instale o Gerenciador de Mods novamente.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.DontDoThat">Hm... não, você não quer fazer isso. :)</system:String>
	
	<system:String x:Key="MessageWindow.Errors.InvalidFramerate.Title">Configuração Inválida</system:String>
	<system:String x:Key="MessageWindow.Errors.InvalidFramerate">Configuração de taxa de quadros inválida detectada.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.CodesListFailed.Title">Erro Carregando Código</system:String>
	<system:String x:Key="MessageWindow.Errors.CodesListFailed">Erro carregando lista de códigos.</system:String>
	
	<system:String x:Key="MessageWindow.Errors.CheckUpdateError.Title">Erros de Atualização</system:String>
	<system:String x:Key="MessageWindow.Errors.CheckUpdateError" xml:space="preserve">Os seguintes erros ocorrem durante a verificação de atualizações:&#x0a;&#x0a;</system:String>

    <system:String x:Key="MessageWindow.Errors.MissingManifest.Title">Mod faltando.manifest</system:String>
    <system:String x:Key="MessageWindow.Errors.MissingManifest">Nenhum dos mods selecionados possuem manifestos, logo não podem ser verificados.</system:String>

    <system:String x:Key="MessageWindow.Errors.ModFailedVerif.Title">Falha de Integridade</system:String>
    <system:String x:Key="MessageWindow.Errors.ModFailedVerif0" xml:space="preserve">Os seguintes mods falharam a verificação:&#x0a;</system:String>
    <system:String x:Key="MessageWindow.Errors.ModFailedVerif1">arquivo(s)</system:String>
    <system:String x:Key="MessageWindow.Errors.ModFailedVerif2" xml:space="preserve">&#x0a;&#x0a;Você deseja efetuar reparos?</system:String>

    <system:String x:Key="MessageWindow.Errors.URIParseFail.Title">Falha analisando URI</system:String>
    <system:String x:Key="MessageWindow.Errors.URIParseFail0">URI de Instalação One-Click Malformado</system:String>
    <system:String x:Key="MessageWindow.Errors.URIParseFail1" xml:space="preserve">causou falha de análise:&#x0a;</system:String>

    <system:String x:Key="MessageWindow.Errors.GBAPIFail.Title">Falha da API do GameBanana</system:String>
    <system:String x:Key="MessageWindow.Errors.GBAPIFail">Consulta da API do Gamebana falhou:</system:String>

    <system:String x:Key="MessageWindow.Errors.URIParseFailFields0">URI de One-Click Install</system:String>
    <system:String x:Key="MessageWindow.Errors.URIParseFailFields1">não conteve os campos necessários</system:String>

    <system:String x:Key="MessageWindow.Errors.DirectoryDeleteFail.Title">Exclusão de Diretório Falhou</system:String>
    <system:String x:Key="MessageWindow.Errors.DirectoryDeleteFail0">Falha em remover pasta temporária de atualização.</system:String>
    <system:String x:Key="MessageWindow.Errors.DirectoryDeleteFail1">Você deseja tentar novamente? Você pode remover a pasta manualmente depois.</system:String>

    <system:String x:Key="MessageWindow.Errors.DirectoryCreateFail.Title">Criação de Diretório Falhou</system:String>
    <system:String x:Key="MessageWindow.Errors.DirectoryCreateFail0">Falha em criar pasta temporária de atualização.</system:String>
    <system:String x:Key="MessageWindow.Errors.DirectoryCreateFail1">Você deseja tentar novamente?</system:String>

    <system:String x:Key="MessageWindow.Errors.UpdateFailed.Title">Atualização Falhou"</system:String>
    <system:String x:Key="MessageWindow.Errors.UpdateFailed">Falha ao atualizar:"</system:String>

    <system:String x:Key="MessageWindow.Errors.ModUpdateFailed0">Falha ao atualizar mod."</system:String>
    <system:String x:Key="MessageWindow.Errors.ModUpdateFailed1">Aperte Tentar Novamente para tentar de novo, ou Cancelar para pular este mod."</system:String>

    <system:String x:Key="MessageWindow.Errors.ManagerUpdateFail">Não foi possível recuperar a informação de atualização."</system:String>

    <system:String x:Key="MessageWindow.Errors.Net7Missing">O Gerenciador de Mods SADX não pode rodar sem .NET 7.</system:String>

    <system:String x:Key="MessageWindow.Errors.VCMissing.Title">Faltando VC++</system:String>
    <system:String x:Key="MessageWindow.Errors.VCMissing" xml:space="preserve">Parece que você está faltando algums Pacotes Redistribuíveis do Microsoft Visual C++, certos mods não funcionarão sem eles.&#x0a;&#x0a;Você deseja baixar e instalar eles agora?</system:String>
    <system:String x:Key="MessageWindow.Errors.ModDelete" xml:space="preserve">Failed to delete the mod {0}.&#x0a;Please make sure the files exist and aren't used by another program, then try again.</system:String>
    <system:String x:Key="MessageWindow.Errors.ProfileLoad">An unexpected error occurred while loading profiles, if the problem persists, please report the issue.</system:String>
    <!--#endregion-->
	
	<!--#region Warnings-->
	<system:String x:Key="MessageWindow.Warnings.DeleteMod0" xml:space="preserve">Isto irá desinstalar todos os mods selecionados:&#x0a;&#x0a;</system:String>
    <system:String x:Key="MessageWindow.Warnings.DeleteMod1">Tem certeza que deseja continuar?</system:String>

	<system:String x:Key="MessageWindow.Warnings.DisableSaveRedirect" xml:space="preserve">Você está prestes a desabilitar redirecionamento de save para este mod. Se continuar, qualquer save (jogo salvo) na pasta deste mod será excluído.&#x0a;&#x0a;Você tem certeza que deseja continuar?</system:String>
	
	<system:String x:Key="MessageWindow.Warnings.DeleteProfile">Você tem certeza que quer apagar este perfil?</system:String>
	
	<system:String x:Key="MessageWindow.Warnings.ForceModUpdate" xml:space="preserve">Isto irá forçar todos os mods selecionados a serem completamente reinstalados.&#x0a;&#x0a;Você tem certeza que deseja continuar?</system:String>
	
	<system:String x:Key="MessageWindow.Warnings.GenerateManifest" xml:space="preserve">Isto pode fazer com que DADOS DE USÚARIO DO MOD (ARQUIVOS DE SAVE [JOGO SALVO], ARQUIVOS DE CONFIGURAÇÃO) SEJAM PERDIDOS na próxima atualização! Para prevenir isto, nunca execute isto em mods que você não desenvolveu.&#x0a;&#x0a;Você tem certeza que deseja continuar?</system:String>

    <system:String x:Key="MessageWindow.Warnings.Net7Missing" xml:space="preserve">O Runtime do .NET 7.0 Desktop não está instalado!&#x0a;&#x0a;Você deseja instalar ele agora?</system:String>
    
	<!--#endregion-->
	
	<!--#region Information-->
	<system:String x:Key="MessageWindow.Information.NoAvailableUpdates.Title">Nenhuma Atualização de Mod Encontrada</system:String>
	<system:String x:Key="MessageWindow.Information.NoAvailableUpdates">Mods estão atualizados.</system:String>


    <system:String x:Key="MessageWindow.Information.ModPassedVerif.Title">Passou em Integridade</system:String>
    <system:String x:Key="MessageWindow.Information.ModPassedVerif">Todos os mods selecionados passaram na verificação.</system:String>

    <system:String x:Key="MessageWindow.Information.GameDetected.Title">Game found</system:String>
    <system:String x:Key="MessageWindow.Information.GameDetected" xml:space="preserve">{0} game has been detected at '{1}'&#x0a;&#x0a;Would you like to download and install the Mod Loader?</system:String>
    <system:String x:Key="MessageWindow.Information.SADXSteamDetectedTemp" xml:space="preserve">SADX game folder has been found at '{0}', but it appears to be the Steam version.&#x0a;The Mod Loader can only work with the PC 2004 Version.&#x0a;&#x0a;Please download and install SADX Mod Installer to convert your game to 2004. (Your saves will be converted as well).</system:String>

    <system:String x:Key="MessageWindow.Information.Exception.Title">Include Settings</system:String>
    <system:String x:Key="MessageWindow.Information.Exception.Message">Would you like to copy your Manager and Profile settings to your clipboard to provide in your report?&#x0a;&#x0a;This is not necessary, but it can assist in resolving problems.</system:String>

    <system:String x:Key="MessageWindow.Information.BugReport.Title">Report Bug</system:String>
    <system:String x:Key="MessageWindow.Information.BugReport.Message1">You are about to submit a bug report, please ensure the following:</system:String>
    <system:String x:Key="MessageWindow.Information.BugReport.Message2">Your Manager is up to date.</system:String>
    <system:String x:Key="MessageWindow.Information.BugReport.Message3">Your Mod Loader is up to date.</system:String>
    <system:String x:Key="MessageWindow.Information.BugReport.Message4">All mods are up to date.</system:String>
    <system:String x:Key="MessageWindow.Information.BugReport.Message5">If you have verified the above, then please follow the directions below to submit to the correct repository:</system:String>
    <system:String x:Key="MessageWindow.Information.BugReport.Message6">Any issues with the program you're currently using, please use the Mod Manager button.</system:String>
    <system:String x:Key="MessageWindow.Information.BugReport.Message7">Any issues with the game, such as freezes, game crashes, etc, please use the Mod Loader button.</system:String>
    <!--#endregion-->

</ResourceDictionary>
