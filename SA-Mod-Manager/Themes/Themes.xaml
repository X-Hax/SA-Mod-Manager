<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:SAModManager.Themes">

    <local:ThemeList x:Key="Themes">
		<local:ThemeEntry FileName="DarkTheme" Name="Dark"/>
		<local:ThemeEntry FileName="LightTheme" Name="Light"/>
		<local:ThemeEntry FileName="TelegramTheme" Name="Telegram Night"/>
		<local:ThemeEntry FileName="DarkGreenTheme" Name="Dark Green"/> 
		<local:ThemeEntry FileName="LightGreenTheme" Name="Light Green"/>
	</local:ThemeList>

</ResourceDictionary>
