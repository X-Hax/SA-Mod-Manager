<UserControl x:Class="SAModManager.Elements.MessageElements.Message"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SAModManager.Elements.MessageElements"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
	<Grid x:Name="Grid" Margin="5">
		<TextBlock x:Name="MessageText" Margin="5" TextWrapping="Wrap" HorizontalAlignment="Left" VerticalAlignment="Center"/>
	</Grid>
</UserControl>
