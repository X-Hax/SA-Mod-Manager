<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SAModManager.Languages">

    <local:LanguageList x:Key="Languages">
        <local:LangEntry FileName="en-EN" Name="English"/>
        <local:LangEntry FileName="fr-FR" Name="Français (France)"/>
        <local:LangEntry FileName="es-ES" Name="Español (España)"/>
        <local:LangEntry FileName="es-MX" Name="Español (Mexicano)"/>
        <local:LangEntry FileName="pt-PT" Name="Português (Portugal)"/>
        <local:LangEntry FileName="pt-BR" Name="Português (Brasil)"/>
        <local:LangEntry FileName="ru-RU" Name="Русский (Россия)"/>
        <local:LangEntry FileName="it-IT" Name="Italiano (Italia)"/>
        <!--<local:LangEntry FileName="he-IL" Name="עברית"/>-->
    </local:LanguageList>



</ResourceDictionary>
